{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**"
      ],
      "env": [
        "NEXT_PUBLIC_GA_ID",
        "EMAIL_SERVER",
        "EMAIL_FROM",
        "NEXT_PUBLIC_url",
        "merchant_id",
        "merchant_key",
        "sandbox_merchant_id",
        "sandbox_merchant_key",
        "merchant_passphrase",
        "eskomSePush_key",
        "server_email",
        "server_email_password",
        "twilio_sid",
        "twilio_token",
        "sendgrid_api_key_id",
        "sendgrid_api_key",
        "yoco_secret_key",
        "yoco_public_key",
        "google_service_client_email",
        "google_service_private_key",
        "google_sheet_id",
        "NEXT_PUBLIC_SANITY_PROJECT_ID",
        "NEXT_PUBLIC_SANITY_DATASET",
        "NEXT_PUBLIC_SANITY_API_VERSION"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.local"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.local",
        ".env.development",
        ".env"
      ]
    }
  },
  "globalDependencies": [
    ".env.local"
  ]
}
