{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "plugins": [
      {
        "name": "next"
      }
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
